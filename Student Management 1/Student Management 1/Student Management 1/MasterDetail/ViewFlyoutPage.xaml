<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Student_Management_1.ViewFlyoutPage"
             xmlns:pages="clr-namespace:Student_Management_1"
            NavigationPage.HasNavigationBar="False">
  <FlyoutPage.Flyout>
        <pages:ViewFlyoutPageFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
    <NavigationPage>
      <x:Arguments>
        <pages:ViewFlyoutPageDetail />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>